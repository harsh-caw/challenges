{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": 0,
        "indent": ["error", 4]
    }
}
